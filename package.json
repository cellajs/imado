{
  "name": "@cellajs/imado",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cellajs/imado.git"
  },
  "description": "File handling with TUS for public and private files using AWS Lambda and S3.",
  "author": "Shareworks Solutions BV <info@shareworks.nl>",
  "homepage": "https://imado.eu",
  "keywords": [
    "file handling",
    "tus",
    "aws",
    "s3",
    "lambda",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "postinstall": "npm run build",
    "dev": "tsup --watch --onSuccess \"tsx dist/index.cjs\""
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.11.8"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.501.0",
    "@aws-sdk/cloudfront-signer": "^3.496.0",
    "@tus/file-store": "^1.2.0",
    "@tus/s3-store": "^1.3.0",
    "jsonwebtoken": "^9.0.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "bugs": {
    "url": "https://github.com/cellajs/imado/issues"
  }
}
